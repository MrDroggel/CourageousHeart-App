{
  "compilerOptions": {
    "strict": true,
    "types": ["node", "react", "react-native", "jest"],
    "paths": {
      "@firebase/auth": ["./node_modules/@firebase/auth/dist/index.rn.d.ts"],
      "@/*": [
        "./*"
      ]
    }
  },
  "exclude": [
    ".expo/",
    "node_modules"
  ],
  "extends": "expo/tsconfig.base",
  "include": [
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx",
    ".eslintrc.js",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "declarations.d.ts"
  ],
}
